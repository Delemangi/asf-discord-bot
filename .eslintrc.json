{
  "env": {
    "es2021": true,
    "node": true
  },
  "globals": {
    "NodeJS": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "quote-props": [
      2,
      "always"
    ]
  }
}